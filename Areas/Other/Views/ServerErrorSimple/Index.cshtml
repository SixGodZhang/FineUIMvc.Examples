@{
    ViewBag.Title = "Other/ServerErrorSimple";
    var F = Html.F();
}


@section body {

    @{
        F.PageManager.SimpleError(true);
    }

    @(F.Button().ID("Button5").OnClick(Url.Action("Button5_Click")).Text("服务器异常（500）"))
