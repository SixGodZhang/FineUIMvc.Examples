@{
    ViewBag.Title = "Form/CalendarMinDate";
    var F = Html.F();
}


@section body {

    @(F.Calendar().ID("Calendar1").DateFormatString(ViewBag.Calendar1DateFormatString).SelectedDate(ViewBag.Calendar1SelectedDate).MinDate(ViewBag.Calendar1MinDate).MaxDate(ViewBag.Calendar1MaxDate).OnDateSelect(Url.Action("Calendar1_DateSelect"), new Parameter("selectedDate", "F.ui.Calendar1.getText()")))
    <br>
    <br>

    @(F.Button().ID("Button1").OnClick(Url.Action("Button1_Click")).Text(ViewBag.Button1Text))
    <br>
    <br>

    @(F.Label().ID("labResult"))


}

